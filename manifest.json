{
  "name": "1Window1Group",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "一つのウィンドウが一つのタブグループとして扱えるように切り替え、保存、復元を簡単にします",
  "icons": {
    "48": "icon48.png"
  },
  "background": {
    "scripts": ["groupmanager.js"]
  },
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
